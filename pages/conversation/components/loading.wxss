.rong-loading{
  padding-top: 15rpx;
}
.rong-loading-process{
    width: 40rpx;
    height: 40rpx;
    position: relative;
    margin: 0 auto;
}
.rong-loading-process span{
    display: inline-block;
    width: 8rpx;
    height: 8rpx;
    border-radius: 50%;
    background: #767579;
    position: absolute;
    animation: load 1.04s ease infinite;
}
@keyframes load{
    0%{
        opacity: 1;
    }
    100%{
        opacity: 0.2;
    }
}
.rong-loading-process span:nth-child(1){
    left: 3rpx;
    top: 50%;
    transform: translateY(-50%);
    animation-delay:0.13s;
}
 .rong-loading-process span:nth-child(2){
    left: 6.5rpx;
    top: 6.5rpx;
    animation-delay:0.26s;
} 
.rong-loading-process span:nth-child(3){
    left: 50%;
    top: 1rpx;
    transform: translateX(-50%);
    animation-delay:0.39s;
}
 .rong-loading-process span:nth-child(4){
    top: 6.5rpx;
    right:6.5rpx;
    animation-delay:0.52s;
} 
.rong-loading-process span:nth-child(5){
    right: 1rpx;
    top: 50%;
    transform: translateY(-50%);
    animation-delay:0.65s;
}
 .rong-loading-process span:nth-child(6){
    right: 6.5rpx;
    bottom:6.5rpx;
    animation-delay:0.78s;
} 
.rong-loading-process span:nth-child(7){
    bottom: 1rpx;
    left: 50%;
    transform: translateX(-50%);
    animation-delay:0.91s;
}
 .rong-loading-process span:nth-child(8){
    bottom: 6.5rpx;
    left: 6.5rpx;
    animation-delay:1.04s;
} 